{
  "name": "flooent",
  "version": "2.0.0",
  "description": "Fluent interface to provide an expressive syntax for common manipulations.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "node japaFile.js",
    "copymeta": "cp README.md package.json dist",
